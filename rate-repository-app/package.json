{
  "name": "rate-repository-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "test": "jest",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint ./src/**/*.{js,jsx} App.js --no-error-on-unmatched-pattern"
  },
  "setupFilesAfterEnv": [
    "<rootDir>/setupTests.js"
  ],
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|react-router-native)"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.14.0",
    "@expo/metro-config": "~54.0.14",
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-picker/picker": "2.11.1",
    "dotenv": "^17.2.4",
    "expo": "~54.0.33",
    "expo-status-bar": "~3.0.9",
    "formik": "^2.4.9",
    "graphql": "^16.12.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-web": "^0.21.0",
    "react-router-native": "^6.30.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/eslint-parser": "^7.28.6",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "babel-preset-expo": "^54.0.10",
    "eslint": "^8.57.1",
    "eslint-plugin-jest": "^29.13.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.17",
    "react-refresh": "^0.18.0",
    "react-test-renderer": "^19.1.0"
  },
  "private": true
}
